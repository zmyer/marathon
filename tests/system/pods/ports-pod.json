{
  "id": "/ports-pod",
  "scheduling": {
    "placement": {
      "constraints": [],
      "acceptedResourceRoles": []
    }
  },
  "containers": [
    {
      "name": "health-app1",
      "exec": {
        "command": {
          "shell": "/opt/mesosphere/bin/python -m http.server $ENDPOINT_HTTPENDPOINT"
        }
      },
      "resources": {
        "cpus": 0.5,
        "mem": 128,
        "disk": 0,
        "gpus": 0
      },
      "endpoints": [
        {
          "name": "httpendpoint",
          "containerPort": null,
          "hostPort": 0,
          "protocol": [
            "tcp"
          ],
          "labels": {}
        }
      ],
      "healthCheck": {
        "http": {
          "endpoint": "httpendpoint",
          "path": "/",
          "scheme": "http"
        },
        "gracePeriodSeconds": 2,
        "intervalSeconds": 2,
        "maxConsecutiveFailures": 2,
        "timeoutSeconds": 1
      }
    },
    {
      "name": "health-app2",
      "exec": {
        "command": {
          "shell": "/opt/mesosphere/bin/python -m http.server $ENDPOINT_HTTPENDPOINT2"
        }
      },
      "resources": {
        "cpus": 0.5,
        "mem": 128,
        "disk": 0,
        "gpus": 0
      },
      "endpoints": [
        {
          "name": "httpendpoint2",
          "hostPort": 0,
          "protocol": [
            "http"
          ],
          "labels": {}
        }
      ],
      "healthCheck": {
        "http": {
          "endpoint": "httpendpoint2",
          "path": "/",
          "scheme": "http"
        },
        "gracePeriodSeconds": 2,
        "intervalSeconds": 2,
        "maxConsecutiveFailures": 2,
        "timeoutSeconds": 1
      }
    }
  ],
  "networks": [
    {
      "name": null,
      "mode": "host",
      "labels": {}
    }
  ]
}
