{
  "id": "/container-net-pod",
  "containers": [
    {
      "name": "nginx",
      "resources": {
        "cpus": 1,
        "mem": 128,
        "disk": 0,
        "gpus": 0
      },
      "image": {
        "kind": "DOCKER",
        "id": "nginx"
      },
      "endpoints": [
        {
          "name": "web",
          "containerPort": 80,
          "protocol": [
            "http"
          ]
        }
      ]
    }
  ],
  "networks": [
    {
      "mode": "container"
    }
  ]
}
